{
  "properties": {
    "owner": "tester",
    "run_strategy": "sequential"
  },
  "workflow": {
    "id": "sample-shell-wf",
    "name": "Test Shell job template workflow",
    "description": "Long description about this workflow",
    "params": {
      "job_template_version":  {
        "value": "v1",
        "type":  "STRING"
      }
    },
